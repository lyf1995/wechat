<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.lyf.wechat.dao.RebateDao">

    <update id="updateRebate">
        UPDATE t_rebate SET regist_rebate = #{rebate.registRebate},buy_rebate = #{rebate.buyRebate} WHERE id = 1
    </update>

    <select id="getRebateInfo" resultType="cn.com.lyf.wechat.entity.Rebate">
        select * from t_rebate where id = 1
    </select>

</mapper>